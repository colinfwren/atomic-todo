{
  "name": "@atomic-todo/test-reporter",
  "version": "1.0.0",
  "description": "Jest Test Reporter that supports annotating tests with Test Case IDs",
  "author": {
    "name": "Colin Wren"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js"
    },
    "./node": {
      "require": "./dist/node.js"
    },
    "./jsdom": {
      "require": "./dist/jsdom.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "jest",
    "reporter"
  ],
  "scripts": {
    "build": "rimraf dist && tsc"
  },
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@jest/expect": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0"
  }
}
